<p style="color: green"><%= notice %></p>
<div class="container text-center">
  <h1><u>Contacts</u></h1>
  <div>
    <% @people.each do |person| %>
      <li style="margin-bottom: 20px">
        <%= link_to person.first_name, person, :class => "btn btn-secondary"  %>
      </li>
    <% end %>
    <%= link_to "New contact", new_person_path,:class => "btn btn-success" %>
  </div>
</div>
